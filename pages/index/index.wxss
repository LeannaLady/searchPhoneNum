.num-wrap{
	margin-bottom: 50rpx;
	position:relative;
}
.num-box{
	width: 570rpx;
	height: 80rpx;
	border:1px solid #ddd;
	border-radius: 12rpx;
	padding-left: 30rpx;
	font-size: 28rpx;
}
.search-btn{
	margin-top: 20rpx;
	font-size: 30rpx;
	color:#3e3a3e;
	height: 80rpx;
	border-radius: 12rpx;
}
.into-item{
	width: 570rpx;
	height: 80rpx;
	font-size: 30rpx;
	line-height: 80rpx;
	background: #e8f8fb;
	padding-left: 30rpx;

}
.close-num{
	position:absolute;
	top:9px;
	right:8px;
	font-size:50rpx;
	color:#ddd;
	z-index:999;

}
